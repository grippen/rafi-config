{
  "title": "Rafi Media Controls",
  "rules": [
    {
      "description": "Rafi Volume Controls (cmd + up/down to volume up/down)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "down_arrow",
            "modifiers": { "mandatory": [ "command" ] }
          },
          "to": [ { "key_code": "volume_decrement" } ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "up_arrow",
            "modifiers": { "mandatory": [ "command" ]
            }
          },
          "to": [ { "key_code": "volume_increment" } ]
        }
      ]
    },
    {
      "description": "Rafi Media Keys (cmd + left/right to rewind/fastforward)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_arrow",
            "modifiers": { "mandatory": [ "command" ] }
          },
          "to": [ { "key_code": "rewind" } ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "right_arrow",
            "modifiers": { "mandatory": [ "command" ] }
          },
          "to": [ { "key_code": "fastforward" } ]
        }
      ]
    }
  ]
}
